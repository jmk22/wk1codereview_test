<body>
  <h1>Contacts</h1>
  @if (@Model.Count == 0) {
    <h3>No contacts have been added yet.</h3>
  }
  @foreach (var contact in Model) {
    <h3>@contact.GetName()</h3>
    <h3>@contact.GetPhoneNumber()</h3>
    <h3>@contact.GetAddress()</h3>
  }
  <p><a href="/">Back home</a></p>
  <h4><a href="/contacts/new">Add a contact</a></h4>
</body>
